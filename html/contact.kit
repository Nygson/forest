<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Forest</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@400;500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="./dist/css/hamburger.min.css">
    <link rel="stylesheet" href="./dist/css/main.min.css">
</head>

<body>
      <!-- @include 'nav.kit' -->
      <div class="nav-margin"></div>
      <section class="contact section-padding">
        <h2 class="title">Kontakt</h2>
        <div class="wrapper">
            <form class="contact__form">

                <div class="contact__form-box">
                    <label for="name">Your name:</label>
                    <input type="text" id="name">
                </div>
                <div class="contact__form-box">
                    <label for="mail">Your email:</label>
                    <input type="email" id="mail">
                </div>
                <div class="contact__form-box">
                    <label for="msg">Message:</label>
                    <textarea id="msg" cols="30" rows="10"></textarea>
                </div>

                <button type="submit" class="contact__form-btn">Send</button>

            </form>
            <iframe
                        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.8952530760016!2d19.890611315612585!3d50.088247979426995!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sMcDonald&#39;s!5e0!3m2!1sen!2spl!4v1612519945713!5m2!1sen!2spl"
                        class="map-frame" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false"
                        tabindex="0" title="Mapa Google McDonald's Jasnogórska 2, 31-342 Kraków"></iframe>
        </div>
      </section>
       <!-- @include 'footer.kit' -->
       <script src="./dist/js/main.min.js"></script>
</body>

</html>